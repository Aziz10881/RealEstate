@page "/houses"
@using RealEstate.Client.Components.HouseTable
@using RealEstate.Client.Components

<div class="row">
    <div class="col-md-5">
        <Search OnSearchChanged="SearchChanged" />
    </div>
    <div class="col-md-5">
        <Sort OnSortChanged="SortChanged" />
    </div>
    <div class="col-md-2">
        <a href="/createHouse">Create House</a>
    </div>
</div>
<div class="row">
    <div class="col">
        <HouseTable Houses="HouseList" OnDeleted="DeleteHouse"/>
    </div>
</div>
<div class="row">
    <div class="col">
        <Page MetaData="MetaData" Spread="2" SelectedPage="SelectedPage" />
    </div>
</div>